{
  "name": "rsvme",
  "version": "1.0.0",
  "description": "RSVP",
  "main": "index.js",
  "dependencies": {
    "@sendgrid/mail": "^7.1.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.9.15",
    "multer": "^1.4.2",
    "qrcode": "^1.4.4",
